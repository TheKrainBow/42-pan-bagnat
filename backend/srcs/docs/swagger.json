    "definitions": {
        "modules.Module": {
            "description": "API Module model",
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "module_01HZ0MMK4S6VQW4WPHB6NZ7R7X"
                },
                "last_update": {
                    "type": "string",
                    "example": "2025-02-18T15:00:00Z"
                },
                "late_commits": {
                    "type": "integer",
                    "example": 2
                },
                "latest_version": {
                    "type": "string",
                    "example": "1.7"
                },
                "name": {
                    "type": "string",
                    "example": "captain-hook"
                },
                "status": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/modules.ModuleStatus"
                        }
                    ],
                    "example": "enabled"
                },
                "git_url": {
                    "type": "string",
                    "example": "https://github.com/some-user/some-repo"
                },
                "version": {
                    "type": "string",
                    "example": "1.2"
                }
            }
        },
        "modules.ModulePatchInput": {
